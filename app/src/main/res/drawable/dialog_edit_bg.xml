<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="1.0px"
    android:insetRight="1.0px">

    <selector>
        <item>
            <shape>
                <corners
                    android:bottomLeftRadius="2.0dip"
                    android:bottomRightRadius="2.0dip"
                    android:topLeftRadius="2.0dip"
                    android:topRightRadius="2.0dip" />

                <stroke
                    android:width="1dp"
                    android:color="#f2f2f2" />
                <solid android:color="#f2f2f2" />
            </shape>
        </item>
    </selector>

</inset>
